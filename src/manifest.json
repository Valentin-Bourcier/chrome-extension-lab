{
    "name": "Chrome extension lab",
    "version": "1.0.0",
    "description": "A technical exercise of a chrome extension creation",
    "manifest_version": 2,
    "permissions": ["tabs", "activeTab", "storage"],
    "background": {
        "scripts": ["runtime.js"]
    },
    "browser_action": {
        "default_popup": "index.html"
    }
}
